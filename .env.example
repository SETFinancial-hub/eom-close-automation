# QuickBooks Online OAuth 2.0 Credentials
# Copy this file to .env and fill in your values.
# NEVER commit .env to git.

QBO_CLIENT_ID=your_client_id_here
QBO_CLIENT_SECRET=your_client_secret_here
QBO_REDIRECT_URI=https://setfinancial-hub.github.io/eom-close-automation/disconnect
QBO_REALM_ID=your_realm_id_here

# Token storage (populated after first OAuth flow)
QBO_ACCESS_TOKEN=
QBO_REFRESH_TOKEN=
QBO_TOKEN_EXPIRY=

# Environment: sandbox | production
QBO_ENVIRONMENT=sandbox

# MCP Server settings
MCP_HOST=127.0.0.1
MCP_PORT=8765
